    "Tanzanian exam results",
    "Tanzanian school system",
    "NECTA data access",
    "Education data management",
    "Tanzanian educational statistics"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Leo",
      "email": "leoalex175@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "symfony/http-client": "^6.2",
    "symfony/browser-kit": "^6.2",
    "symfony/css-selector": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "NectaResultScraper\\": "src/"
    }
  }
}
